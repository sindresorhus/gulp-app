{
  "name": "gulp-app",
  "productName": "Gulp",
  "version": "0.0.1",
  "description": "Gulp as an app (macOS)",
  "license": "MIT",
  "repository": "sindresorhus/gulp-app",
  "author": {
    "name": "Sindre Sorhus",
    "email": "sindresorhus@gmail.com",
    "url": "sindresorhus.com"
  },
