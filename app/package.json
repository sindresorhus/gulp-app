    "get-gulp-tasks": "^4.0.0",
    "gulp": "^3.6.2",
    "lodash": "^4.13.1",
    "pkg-up": "^1.0.0"
  },
  "devDependencies": {
    "electron": "^1.3.3",
    "electron-packager": "^8.5.1",
    "xo": "*"
